<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StockProductorCF.Vistas.ListaHojasCalculoGoogle"
             Title="Productor de la CF - Inventario">

  <ContentPage.Content>
    <ScrollView>
      <StackLayout Spacing="0" Padding="0">
        <StackLayout BackgroundColor="#c41d23" Spacing="0" Padding="0" Orientation="Horizontal" VerticalOptions="Start" HorizontalOptions="Fill">
          <Image x:Name="Cabecera" WidthRequest="149" HeightRequest="53" VerticalOptions="Start" HorizontalOptions="EndAndExpand"/>
        </StackLayout>
        <StackLayout BackgroundColor="Gray" VerticalOptions="Start" HorizontalOptions="Fill">
          <Label FontSize="18" FontAttributes="Bold" TextColor="White" HorizontalOptions="Center">Seleccione una hoja</Label>
        </StackLayout>
        <StackLayout x:Name="ContenedorHojas" Spacing="4" Padding="4">
        </StackLayout>
      </StackLayout>
    </ScrollView>
  </ContentPage.Content>

</ContentPage>