<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StockProductorCF.PaginaAuntenticacion"
						 NavigationPage.HasNavigationBar="false">

	<ContentPage.Content>
		<ScrollView>
			<StackLayout Spacing="0" Padding="0">
				<StackLayout VerticalOptions="Start" HorizontalOptions="Fill" Spacing="0" Padding="0">
					<Image x:Name="Cabecera" VerticalOptions="Start" HorizontalOptions="FillAndExpand"/>
				</StackLayout>
				<StackLayout BackgroundColor="#C0C0C0" VerticalOptions="Start" HorizontalOptions="Fill">
					<Label FontSize="18" FontAttributes="Bold" TextColor="Black" HorizontalOptions="Center" HeightRequest="30">Autenticación en Google</Label>
				</StackLayout>
				<StackLayout x:Name="Contenedor" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand" Spacing="0" Padding="0">
				</StackLayout>
			</StackLayout>
		</ScrollView>
	</ContentPage.Content>
	
</ContentPage>