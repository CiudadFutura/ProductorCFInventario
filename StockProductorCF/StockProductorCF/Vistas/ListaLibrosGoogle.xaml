<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StockProductorCF.Vistas.ListaLibrosGoogle"
             Title="Productor de la CF - Inventario">

  <ContentPage.Content>
    <ScrollView>
      <StackLayout Spacing="0" Padding="0">
        <StackLayout BackgroundColor="Gray" VerticalOptions="Start" HorizontalOptions="Fill">
          <Label FontSize="18" FontAttributes="Bold" TextColor="White" HorizontalOptions="Center">Seleccione un libro</Label>
        </StackLayout>
        <StackLayout x:Name="ContenedorLibros" Spacing="5" Padding="5" VerticalOptions="FillAndExpand" HorizontalOptions="Fill">
        </StackLayout>
      </StackLayout>
    </ScrollView>
  </ContentPage.Content>

</ContentPage>